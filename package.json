{
  "name": "app-example",
  "version": "0.2.0",
  "description": "example usage of @serverless-guy/lambda",
  "main": "index.js",
  "scripts": {
    "test": "npm run coverage",
    "setup": "npm i && cd layers/dependencies/nodejs/node8 && npm i",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:mocha",
    "test:mocha": "NODE_PATH=\"./node_modules:./layers/dependencies/nodejs/node8/node_modules\" mocha-webpack test/**/*.test.js --reporter mocha-junit-reporter --reporter-options mochaFile=./test-reports/junit.xml",
    "test:mocha-no-junit": "NODE_PATH=\"./node_modules:./layers/dependencies/nodejs/node8/node_modules\" mocha-webpack test/**/*.test.js",
    "run:dev": "sls offline --stage dev",
    "dev": "NODE_PATH=\"./node_modules:./layers/dependencies/nodejs/node8/node_modules\" npm run run:dev"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-syntax-decorators": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "aws-sdk": "^2.384.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "istanbul-instrumenter-loader": "^3.0.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "mocha-webpack": "^2.0.0-beta.0",
    "nock": "^10.0.6",
    "nyc": "^13.1.0",
    "serverless": "^1.36.1",
    "serverless-offline": "^3.33.0",
    "serverless-prune-plugin": "^1.3.2",
    "serverless-webpack": "^5.2.0",
    "source-map-support": "^0.5.9",
    "webpack": "^4.28.3",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {}
}
